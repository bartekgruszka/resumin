{% include "@ResuminAdmin/Partials/_header.html.twig" %}
<div class="row">
    <div class="col-md-6 col-md-offset-3">
        <form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="form-horizontal fos_user_registration_register">
            {{ form_widget(form) }}
            <div class="form-group">
                <div class="col-sm-offset-2 col-sm-10">
                    <button type="submit"class="btn btn-primary">{{ 'registration.submit'|trans({}, 'FOSUserBundle') }}</button>
                </div>
            </div>
        </form>
    </div>
</div>

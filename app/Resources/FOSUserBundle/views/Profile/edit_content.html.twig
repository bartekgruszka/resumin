{% include "@ResuminAdmin/Partials/_header.html.twig" %}
<div class="row">
    <div class="col-md-6 col-md-offset-3">
        <form action="{{ path('fos_user_profile_edit') }}" {{ form_enctype(form) }} method="POST" class="fos_user_profile_edit">
            {{ form_widget(form) }}
            <div class="form-group">
                <a href="{{ path('fos_user_profile_show') }}" class="btn btn-default">
                    Back
                </a>
                <button type="submit" class="btn btn-primary pull-right">
                    {{ 'profile.edit.submit'|trans({}, 'FOSUserBundle') }}
                </button>
            </div>
        </form>
    </div>
</div>
<div class="row">
    <div class="col-md-6 col-md-offset-3">
        <a href="{{ path('fos_user_change_password') }}" class="btn btn-primary pull-right">
            {{ 'change_password.submit'|trans({}, 'FOSUserBundle') }}
        </a>
    </div>
</div>
